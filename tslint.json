{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-plugin-prettier",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "prettier": [
      true,
      { "singleQuote": true, "trailingComma": "all", "parser": "typescript" }
    ],
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "prefer-object-spread": true
  },
  "rulesDirectory": []
}
